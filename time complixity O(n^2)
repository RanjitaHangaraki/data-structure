#include <stdio.h>
#include <time.h>

int main() {
    int n = 1000, i, j, temp;
    int arr[n];
    for (i = 0; i < n; i++)
        arr[i] = n - i;

    clock_t start = clock(); // start time

    // Bubble sort
    for (i = 0; i < n - 1; i++) {
        for (j = 0; j < n - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }

    clock_t end = clock(); // end time
    double time_taken = ((double)(end - start)) / CLOCKS_PER_SEC;

    printf("Bubble Sort executed in %f seconds\n", time_taken);
    return 0;
}
